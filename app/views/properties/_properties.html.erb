<% properties.each do |p| %>
    <div style="padding-top: 25px; padding-bottom: 25px; padding-left: 5%; border-style: solid; border-width: 1px; border-color: gray">
    <h3><%= link_to p.title, property_path(p) %></h4>
    <p><%= p.description %></p>
    <p>Quartos: <%= p.rooms %></p>
    </div>
<% end %>