<h1>Flats</h1>
<h3>Boas vindas ao sistema de locação de apartamentos</h3>

<% @properties.each do |p| %>
    <%= link_to p.title, property_path(p) %>
    <%= p.description %>
     Quartos: <%= p.rooms %>
<% end %>

<% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
<% end %>

<%= link_to 'Cadastrar imóvel', new_property_path %>